#include <stdio.h>
void odd_even(int arr[], int n) {
    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) {
            printf("%d is even\n", arr[i]);
        } else {
            printf("%d is odd\n", arr[i]);
        }
    }
}
int main() {
    int arr[] = {12, 7, 5, 20, 13};
    int n = sizeof(arr) / sizeof(arr[0]);
    odd_even(arr, n);
    return 0;
}
